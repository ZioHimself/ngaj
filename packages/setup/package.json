    "./utils/script-validator": "./dist/utils/script-validator.js",
    "./utils/script-validator.js": "./dist/utils/script-validator.js",
    "./utils/lan-ip-detection": "./dist/utils/lan-ip-detection.js",
    "./utils/lan-ip-detection.js": "./dist/utils/lan-ip-detection.js",
    "./utils/login-code-reader": "./dist/utils/login-code-reader.js",
    "./utils/login-code-reader.js": "./dist/utils/login-code-reader.js",
    "./utils/shutdown-handler": "./dist/utils/shutdown-handler.js",
    "./utils/shutdown-handler.js": "./dist/utils/shutdown-handler.js",
    "./utils/docker-manager": "./dist/utils/docker-manager.js",
    "./utils/docker-manager.js": "./dist/utils/docker-manager.js",
    "./handlers/signal-handler": "./dist/handlers/signal-handler.js",
    "./handlers/signal-handler.js": "./dist/handlers/signal-handler.js",
    "./writers/env-writer": "./dist/writers/env-writer.js",
    "./writers/env-writer.js": "./dist/writers/env-writer.js",
