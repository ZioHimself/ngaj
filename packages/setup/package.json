    "./validators/bluesky": "./dist/validators/bluesky.js",
    "./validators/bluesky.js": "./dist/validators/bluesky.js"
  },
  "bin": {
    "ngaj-setup": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "type-check": "tsc -p tsconfig.json --noEmit",
    "clean": "rm -rf dist",
    "docker:build": "docker build -t ngaj/setup -f Dockerfile ../.."
